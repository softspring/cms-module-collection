{
    "name": "softspring/cms-module-collection",
    "description": "A module collection for softspring/cms-bundle",
    "type": "library",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=8.1",
        "softspring/cms-bundle": "^5.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.14",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^10.2.1"
    },
    "config": {
        "sort-packages": true
    },
    "autoload-dev": {
        "psr-4": {
            "Softspring\\CmsModuleCollection\\Test\\Modules\\": "tests/modules"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-5.0": "5.0.x-dev",
            "dev-5.1": "5.1.x-dev"
        }
    }
}
